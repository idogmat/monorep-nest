datasource db {
  provider = "postgresql"
  url      = env("PROFILE_DB_URL")
}

generator client {
  provider = "prisma-client-js"
  output   = "../../../node_modules/.prisma/profile-client"
}

model Profile {
  id              String    @id @default(uuid())
  userId          String    @unique
  photoUrl        String?
  email           String
  paymentAccont   Boolean   @default(false)
  userName        String
  aboutMe         String?   @db.VarChar(200)
  firstName       String?   
  lastName        String?
  dateOfBirth     DateTime?
  country         String?
  city            String?

  @@index([userName])
  @@index([firstName, lastName])
  @@map("profiles")
}