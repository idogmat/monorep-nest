datasource db {
  provider = "postgresql"
  url      = env("PROFILE_DB_URL")
}

generator client {
  provider = "prisma-client-js"
  output   = "../../../node_modules/.prisma/profile-client"
}

model Profile {
  id        String   @id @default(uuid())
  userId    String   @unique
  photoUrl  String
  firstName String?
  lastName String?
  @@map("profiles")
}

