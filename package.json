    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "crypto": "^1.0.1",
    "dataloader": "^2.2.3",
    "date-fns": "^4.1.0",
    "dotenv-cli": "^8.0.0",
    "form-data": "^4.0.2",
    "google-auth-library": "^9.15.1",
    "google-proto-files": "^4.2.0",
    "graphql": "^16.10.0",
    "grpc-tools": "^1.13.0",
    "ioredis": "^5.6.0",
    "mongoose": "^8.12.2",
    "multer": "^1.4.5-lts.2",
    "nodemailer": "^6.10.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.5",
    "socket.io": "^4.8.1",
