{
  "name": "monorep",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "inspect": "node --inspect-brk ./node_modules/.bin/nest start gateway",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/gateway/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "dotenv -e .env.test -- npx prisma generate --schema=apps/gateway/prisma/schema.prisma && dotenv -e .env.test -- npx prisma migrate dev --schema=apps/gateway/prisma/schema.prisma && dotenv -e .env.test -- jest --config ./apps/gateway/test/jest-e2e.json",
    "prisma": "node prisma-script.js",
    "generate:proto": "ts-node generate-proto.ts",
    "rabbitmq": "docker-compose -f docker-compose.rabbitmq.yaml up -d",
    "redis": "docker-compose -f docker-compose.redis.yaml up -d",
    "redis:terminal": "docker exec -it redis redis-cli",
    "seed": "ts-node apps/profile/src/features/prisma/seed.countries.prisma.ts",
    "build:gateway": "nest build gateway",
    "start:gateway": "node dist/apps/gateway/main",
    "start:dev:gateway": "nest start gateway --watch",
    "build:files": "nest build files",
    "start:files": "node dist/apps/files/main",
    "start:dev:files": "nest start files --watch",
    "build:profile": "nest build profile",
    "start:profile": "node dist/apps/profile/main",
    "start:dev:profile": "nest start profile --watch",
    "build:payments": "nest build payments",
    "start:payments": "node dist/apps/payments/main",
    "start:dev:payments": "nest start payments --watch",
    "build:posts": "nest build posts",
    "start:posts": "node dist/apps/posts/main",
    "start:dev:posts": "nest start posts --watch"
  },
  "dependencies": {
    "@apollo/server": "^4.12.0",
